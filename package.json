{
	"name": "nino",
	"version": "2.0.1",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Sebazz & Nino Team (https://github.com/Nino-bot)",
	"license": "Apache-2.0",
	"main": "dist/main.js",
	"imports": {
		"@/*": "./dist/*.js"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/discord.js-utilities": "^4.9.5",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "^3.0.0-next.fd7be0b.0",
		"@sapphire/plugin-i18next": "^2.5.0",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/type": "^2.2.0",
		"@sapphire/utilities": "^3.6.2",
		"colorette": "^2.0.16",
		"delay": "^5.0.0",
		"discord.js": "latest",
		"mongoose": "^6.3.1",
		"ts-dotenv": "^0.8.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.171",
		"@typescript-eslint/eslint-plugin": "5.20.0",
		"@typescript-eslint/parser": "5.20.0",
		"eslint": "8.13.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.0.0",
		"tsc-watch": "^5.0.3",
		"typescript": "4.6.3"
	},
	"scripts": {
		"start": "node ./dist/main.js",
		"build": "swc src --copy-files  -d dist",
		"dev": "tsc-watch --onSuccess \"node dist/main.js\"",
		"clean": "node scripts/clean.mjs",
		"eslint-err": "eslint src/ --ext .ts --cache",
		"eslint-fix": "eslint src/ --fix --ext .ts --cache"
	},
	"packageManager": "yarn@3.2.0"
}
