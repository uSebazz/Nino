{
	"name": "nino",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Sebazz",
	"version": "2.0.3",
	"license": "Apache-2.0",
	"type": "module",
	"main": "dist/Nino.js",
	"scripts": {
		"build": "tsup src && node scripts/i18n.mjs",
		"clean": "node scripts/clean.mjs",
		"lang": "node scripts/i18n.mjs",
		"start": "node --enable-source-maps ./dist/Nino.js",
		"dev": "tsc-watch --onSuccess \"node --enable-source-maps ./dist/Nino.js\"",
		"deploy": "yarn build && yarn start",
		"style": "eclint fix 'src/**/*'"
	},
	"packageManager": "yarn@3.2.1",
	"dependencies": {
		"@colors/colors": "^1.5.0",
		"@prisma/client": "3.15.2",
		"@sapphire/decorators": "4.3.6",
		"@sapphire/discord.js-utilities": "4.11.3",
		"@sapphire/fetch": "2.4.0",
		"@sapphire/framework": "3.0.0-next.fd7be0b.0",
		"@sapphire/plugin-editable-commands": "1.2.0",
		"@sapphire/plugin-i18next": "2.5.2",
		"@sapphire/plugin-logger": "2.2.1",
		"@sapphire/type": "2.2.4",
		"@sapphire/utilities": "3.6.2",
		"@skyra/env-utilities": "1.0.0",
		"@skyra/start-banner": "^1.0.0",
		"discord.js": "13.8.0",
		"gradient-string": "^2.0.1"
	},
	"devDependencies": {
		"@types/gradient-string": "^1.1.2",
		"@types/node": "17.0.35",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"eclint": "^2.8.1",
		"eslint": "^8.18.0",
		"prisma": "3.15.2",
		"tsc-watch": "5.0.3",
		"tsup": "6.1.2",
		"typescript": "4.7.4"
	},
	"imports": {
		"#utils/*": "./dist/lib/utils/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/i18n": "./dist/lib/i18n/index.js",
		"#lib/i18n/Language": "./dist/lib/i18n/Language/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#root/*": "./dist/*.js"
	}
}
