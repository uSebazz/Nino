{
	"name": "nino",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Sebazz",
	"version": "2.0.4",
	"license": "Apache-2.0",
	"type": "module",
	"main": "dist/Nino.js",
	"scripts": {
		"build": "tsup src",
		"clean": "node scripts/clean.mjs",
		"start": "node --preserve-symlinks --enable-source-maps ./dist/Nino.js",
		"prisma": "prisma generate",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,json}\"",
		"dev": "tsc-watch --onSuccess \"yarn start\"",
		"deploy": "yarn build && yarn start"
	},
	"packageManager": "yarn@3.2.1",
	"dependencies": {
		"@kaname-png/plugin-statcord": "2.1.0",
		"@kaname-png/plugin-subcommands-advanced": "^1.2.1",
		"@prisma/client": "4.0.0",
		"@sapphire/decorators": "4.3.7",
		"@sapphire/discord.js-utilities": "4.11.3",
		"@sapphire/fetch": "2.4.0",
		"@sapphire/framework": "3.0.0-next.fd7be0b.0",
		"@sapphire/plugin-editable-commands": "1.2.1",
		"@sapphire/plugin-i18next": "2.5.2",
		"@sapphire/plugin-logger": "2.2.2",
		"@sapphire/plugin-subcommands": "pr-271",
		"@sapphire/type": "2.2.4",
		"@sapphire/utilities": "3.7.0",
		"@skyra/env-utilities": "1.0.1",
		"@skyra/start-banner": "1.0.1",
		"discord.js": "13.8.1",
		"gradient-string": "2.0.1"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "17.0.3",
		"@sapphire/eslint-config": "4.3.7",
		"@sapphire/prettier-config": "1.4.3",
		"@types/gradient-string": "1.1.2",
		"@types/ms": "0.7.31",
		"@types/node": "18.0.0",
		"@types/prettier": "2",
		"@typescript-eslint/eslint-plugin": "5.30.0",
		"@typescript-eslint/parser": "5.30.0",
		"commitlint": "17.0.3",
		"eslint": "^8.18.0",
		"prettier": "2.7.1",
		"prisma": "^4.0.0",
		"tsc-watch": "5.0.3",
		"tsup": "6.1.2",
		"typescript": "4.7.4"
	},
	"imports": {
		"#utils/*": "./dist/lib/utils/*.js",
		"#utils/function": "./dist/lib/utils/function/index.js",
		"#utils/sanitizer": "./dist/lib/utils/sanitizer/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/i18n": "./dist/lib/i18n/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#root/*": "./dist/*.js"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"prettier": {
		"endOfLine": "lf",
		"printWidth": 150,
		"quoteProps": "as-needed",
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	}
}
