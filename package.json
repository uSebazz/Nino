{
	"name": "nino",
	"version": "2.0.2",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Sebazz & Nino Team (https://github.com/Nino-bot)",
	"license": "Apache-2.0",
	"main": "dist/Nino.js",
	"imports": {
		"#class/*": "./dist/class/*.js",
		"#utils/*": "./dist/lib/utils/*.js",
		"#root/*": "./dist/*.js"
	},
	"dependencies": {
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/discord.js-utilities": "^4.10.0",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "next",
		"@sapphire/plugin-editable-commands": "^1.2.0",
		"@sapphire/plugin-i18next": "^2.5.0",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"axios": "^0.27.2",
		"colorette": "^2.0.16",
		"discord.js": "^13.6.0",
		"mongoose": "^6.3.2",
		"ts-dotenv": "^0.8.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.178",
		"@typescript-eslint/eslint-plugin": "5.22.0",
		"@typescript-eslint/parser": "5.22.0",
		"eslint": "8.15.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.0.0",
		"prettier": "2.6.2",
		"tsc-watch": "^5.0.3",
		"typescript": "4.6.4"
	},
	"scripts": {
		"start": "node ./dist/Nino.js",
		"build": "swc src --copy-files  -d dist",
		"dev": "tsc-watch --onSuccess \"node dist/Nino.js\"",
		"clean": "node scripts/clean.mjs",
		"eslint-err": "eslint src/ --ext .ts --cache",
		"eslint-fix": "eslint src/ --fix --ext .ts --cache"
	},
	"packageManager": "yarn@3.2.0"
}
